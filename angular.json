{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng2-quiz": {
      "root": "projects/app-user/",
      "sourceRoot": "projects/app-user/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-user",
            "index": "projects/app-user/src/index.html",
            "main": "projects/app-user/src/main.ts",
            "polyfills": "projects/app-user/src/polyfills.ts",
            "tsConfig": "projects/app-user/src/tsconfig.app.json",
            "assets": [
              "projects/app-user/src/favicon.ico",
              "projects/app-user/src/assets"
            ],
            "styles": [
              "projects/app-user/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app-user/src/environments/environment.ts",
                  "with": "projects/app-user/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng2-quiz:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng2-quiz:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng2-quiz:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app-user/src/test.ts",
            "polyfills": "projects/app-user/src/polyfills.ts",
            "tsConfig": "projects/app-user/src/tsconfig.spec.json",
            "karmaConfig": "projects/app-user/src/karma.conf.js",
            "styles": [
              "projects/app-user/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/app-user/src/favicon.ico",
              "projects/app-user/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app-user/src/tsconfig.app.json",
              "projects/app-user/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng2-quiz-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng2-quiz:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng2-quiz:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-admin": {
      "root": "projects/app-admin/",
      "sourceRoot": "projects/app-admin/src",
      "projectType": "application",
      "prefix": "app-admin",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-admin",
            "index": "projects/app-admin/src/index.html",
            "main": "projects/app-admin/src/main.ts",
            "polyfills": "projects/app-admin/src/polyfills.ts",
            "tsConfig": "projects/app-admin/tsconfig.app.json",
            "assets": [
              "projects/app-admin/src/favicon.ico",
              "projects/app-admin/src/assets"
            ],
            "styles": [
              "projects/app-admin/src/styles.css",
              "../ng2-quiz/node_modules/font-awesome/css/font-awesome.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app-admin/src/environments/environment.ts",
                  "with": "projects/app-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app-admin/src/test.ts",
            "polyfills": "projects/app-admin/src/polyfills.ts",
            "tsConfig": "projects/app-admin/tsconfig.spec.json",
            "karmaConfig": "projects/app-admin/karma.conf.js",
            "styles": [
              "projects/app-admin/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/app-admin/src/favicon.ico",
              "projects/app-admin/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app-admin/tsconfig.app.json",
              "projects/app-admin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-admin-e2e": {
      "root": "projects/app-admin-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app-admin-e2e/protractor.conf.js",
            "devServerTarget": "app-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/app-admin-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng2-quiz"
}
